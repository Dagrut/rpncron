Use this file to make your own tests.

@DFT .
	env-set HOME /home/sweet/home override
	env-unset HOME
	exec pipe
	mode offset
	max-proc 2
	on-output ignore mail log
	on-error ignore mail log
	on-exec-error ignore mail log
.

@RPN .
3
.

@CMD .
prt_var() {
	val=`eval "echo \\$$1"`
	echo "$1=$val"
}
prt_var FOOBAR > /tmp/rpncron.examples.tests.rct.1
prt_var HOME > /tmp/rpncron.examples.tests.rct.1
prt_var USER > /tmp/rpncron.examples.tests.rct.1
prt_var SHELL > /tmp/rpncron.examples.tests.rct.1
sleep 0.2
exit 3
.

@CFG .
	env-set FOOBAR test
.

@RPN .
3
.

@CMD .
prt_var() {
	val=`eval "echo \\$$1"`
	echo "$1=$val"
}
prt_var FOOBAR > /tmp/rpncron.examples.tests.rct.1
prt_var HOME > /tmp/rpncron.examples.tests.rct.1
prt_var USER > /tmp/rpncron.examples.tests.rct.1
prt_var SHELL > /tmp/rpncron.examples.tests.rct.1
sleep 0.2
exit 3
.
